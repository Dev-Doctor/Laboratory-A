<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.devdoctor.deltabooks.controllers.masterController">
   <left>
      <VBox prefHeight="500.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <Pane prefHeight="500.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="B_login" layoutX="50.0" layoutY="50.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="LOGIN" />
         </children></Pane>
   </center>
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="Bhome" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="HOME" />
            <Separator prefWidth="10.0" />
            <Label text="Home">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
        </items>
      </ToolBar>
   </top>
</BorderPane>
